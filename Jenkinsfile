
// SCRIPTED PIPELINE


// DECLARATIVE PIPELINE

pipeline {
	stages{

stage('Build') {
		echo "Build"
	}
	stage('Unit Test') {
		echo "Unit-Test"
	}
	stage('Integration test'){
		echo "Intregation-Test"
	}
	}
	always{
		echo "Always run"
	}
	success{
		echo "Success run"
	}
	failure{
		echo "Failure run"
	}
	
}
